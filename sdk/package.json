{
  "name": "@alleyboss/ashborn-sdk",
  "version": "2.1.5",
  "description": "The Shadow Monarch SDK - Universal Privacy Layer for Solana with ZK proofs, stealth addresses, and selective disclosure",
  "author": "AlleyBo55",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlleyBo55/ashborn"
  },
  "homepage": "https://ashborn-sol.vercel.app/",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./stealth": {
      "types": "./dist/stealth.d.ts",
      "import": "./dist/stealth.mjs",
      "require": "./dist/stealth.js"
    },
    "./zk": {
      "types": "./dist/zk.d.ts",
      "import": "./dist/zk.mjs",
      "require": "./dist/zk.js"
    },
    "./integrations": {
      "types": "./dist/integrations.d.ts",
      "import": "./dist/integrations.mjs",
      "require": "./dist/integrations.js"
    },
    "./security": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.mjs",
      "require": "./dist/security.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts src/stealth.ts src/zk.ts src/integrations.ts src/security.ts --format cjs,esm --dts --splitting --external @noble/curves --external @noble/hashes --external @solana/web3.js --external @solana/spl-token --external @coral-xyz/anchor --external snarkjs --external privacycash --external poseidon-lite --external bn.js",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@solana/spl-token": "^0.4.0",
    "bn.js": "^5.2.1",
    "poseidon-lite": "^0.2.0"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": ">=0.30.0",
    "@solana/web3.js": ">=1.90.0",
    "privacycash": "*",
    "snarkjs": ">=0.7.0"
  },
  "peerDependenciesMeta": {
    "snarkjs": {
      "optional": true
    },
    "privacycash": {
      "optional": true
    },
    "@coral-xyz/anchor": {
      "optional": true
    }
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "privacycash": "github:Privacy-Cash/privacy-cash-sdk",
    "snarkjs": "^0.7.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk-proofs",
    "stealth-address",
    "shielded-transaction",
    "groth16",
    "poseidon",
    "zero-knowledge",
    "compliance"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
